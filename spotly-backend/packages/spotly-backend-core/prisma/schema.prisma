// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
    output   = "../node_modules/.prisma/client"
}

// The `url` property is deprecated in prisma schema files as of Prisma 7.
// Move connection URLs for migrate to `prisma.config.ts` instead.

datasource db {
    provider = "postgresql"
}

// Example User model - update based on your needs
model ParkingSpot {
    id String @id @default(uuid())

    name        String
    description String?
    latitude    Float
    longitude   Float
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    @@map("parking_spots")
}
