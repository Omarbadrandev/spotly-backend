{
  "name": "@spotly-backend/core",
  "version": "14.1.6",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "5.2.0"
  },
  "devDependencies": {
    "prisma": "5.2.0"
  },
  "scripts": {
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:migrate:apply": "prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "prisma:create-migration-only": "npx prisma migrate dev --schema=prisma/schema.prisma --create-only"
  }
}
