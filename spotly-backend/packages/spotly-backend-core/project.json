{
  "name": "spotly-backend-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/spotly-backend-core/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/packages/spotly-backend-core",
        "tsConfig": "packages/spotly-backend-core/tsconfig.lib.json",
        "packageJson": "packages/spotly-backend-core/package.json",
        "main": "packages/spotly-backend-core/src/index.ts",
        "assets": ["packages/spotly-backend-core/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/spotly-backend-core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/spotly-backend-core/jest.config.ts"
      }
    },
    "prisma:generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema=packages/spotly-backend-core/prisma/schema.prisma",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
