{
  "name": "spotly-backend-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/spotly-backend-e2e/src",
  "projectType": "application",
  "tags": ["e2e"],
  "implicitDependencies": ["spotly-backend"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{projectRoot}/test-output/jest/coverage"],
      "options": {
        "jestConfig": "apps/spotly-backend-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["spotly-backend:build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/spotly-backend-e2e/**/*.ts"]
      }
    }
  }
}
