{
  "name": "@spotly-backend/spotly-backend-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@spotly-backend/spotly-backend"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/spotly-backend-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@spotly-backend/spotly-backend:build",
          "@spotly-backend/spotly-backend:serve"
        ]
      }
    }
  }
}
